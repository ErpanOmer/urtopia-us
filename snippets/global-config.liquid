  function fetchBuried(name = '', type = '', data = {}, isSendBeacon = false) {
    const body = {
      module: 'website-us',
      trace_name: `${name}-us`,
      trace_type: `${type}-${document.body.clientWidth > 768 ? 'pc' : 'mb'}`,
      extras: data,
