<style>
	.order-flex-row {
	  display: flex;
	  align-items: center;
	}
	.order-modal-layer {
	  position: fixed;
	  z-index: 999;
	  top: 0;
	  left: 0;
	  height: 100vh;
	  width: 100%;
	  background-color: rgba(0, 0, 0, 0.5);
	  display: flex;
	  align-items: center;
	  justify-content: center;
	  display: none;
	}
	.order-modal-layer .order-layer-box {
	  opacity: 0;
	  background-color: #fff;
	  border-radius: 15px;
	  width: 900px;
	  box-shadow: 0 0 10px 10px rgb(0, 0, 0, 0.08);
	}
	.order-modal-layer .order-layer-header {
	  padding: 12px 20px;
	  justify-content: space-between;
	  border-bottom: 1px solid #eee;
	}
	.order-modal-layer .order-layer-header h3 {
	  font: normal normal 600 16px/19px urbane;
	  margin: 0;
	  margin-left: 20px;
	}
	.order-modal-layer .order-layer-header span {
	  font-size: 24px;
	  background-color: transparent;
	  outline: none;
	  border: none;
	  cursor: pointer;
	  color: #cfcfcf;
	}
	.order-modal-layer .sizefit-switch {
	  background-color: #eff0f4;
	  border-radius: 50px;
	  width: 50%;
	  margin: 20px auto;
	  overflow: hidden;
	}
	.sizefit-switch .switch-radio {
	  position: relative;
	  width: 100%;
	  overflow: hidden;
	  cursor: pointer;
	}
	.order-modal-layer .sizefit-switch button {
	  padding: 6px 20px;
	  margin: 0;
	  font: normal normal 600 16px/33px urbane;
	  background-color: transparent;
	  outline: none;
	  border: none;
	  cursor: pointer;
	  overflow: hidden;
	  width: 100%;
	  border-radius: 50px;
	  transition: all 0.1s;
	  line-height: 22px;
	}

	.order-modal-layer .sizefit-switch button span {
		font-size: 14px;
    	font-weight: 300;
	}

	.order-modal-layer .sizefit-switch input[type="radio"] {
	  position: absolute;
	  width: 100%;
	  height: 100%;
	  left: 0;
	  top: 0;
	  margin: 0;
	  opacity: 0;
	}
	#tab-s:checked ~ .btn,
	#tab-m:checked ~ .btn,
	#tab-l:checked ~ .btn {
	  background: #000000;
	  color: #fff;
	}
	.order-modal-layer .order-layer-body {
	  padding: 20px 40px;
	  padding-bottom: 40px;
	}
	.sizefit-box {
	  width: 35%;
	}
	.sizefit-item {
	  justify-content: space-between;
	}
	.sizefit-item:not([data-last="yes"]) {
	  margin-bottom: 25px;
	}

	.sizefit-item p {
	  font: normal normal 600 12px/14px urbane;
	  color: #2b2e34;
	  margin: 0;
	}
	.sizefit-item span {
	  font: normal normal 300 12px/14px urbane;
	  color: #2b2e34;
	}
	.sizefit-bg {
	  flex: 1;
	  height: 340px;
	  margin-left: 50px;
	  {% if hide_bala %}
	  background-image: url("https://cdn.shopify.com/s/files/1/0633/2068/6808/files/20230823-113031.png?v=1692761477") ;
	  {% else %}
	  background-image: url("https://cdn.shopify.com/s/files/1/0583/5810/4213/files/20230413-101607.jpg?v=1681352192") ;
	  {% endif %}
	  background-position: center;
	  background-size: auto 100%;
	  background-repeat: no-repeat;
	}
	.sizefitx-bg {
	  background-image: url("https://cdn.shopify.com/s/files/1/0583/5810/4213/files/sizefitx.png?v=1677814083");
	}
	.dialog-open {
	  animation-name: anim-open;
	  animation-duration: 0.3s;
	  animation-fill-mode: forwards;
	}
	.dialog-close {
	  animation-name: anim-close;
	  animation-duration: 0.3s;
	  animation-fill-mode: forwards;
	}

	.dialog-close-out {
	  animation-name: anim-close-out;
	  animation-duration: 0.3s;
	  animation-fill-mode: forwards;
	}

	/* modal2 */
	.tec-spac-t {
	  font: normal normal 600 20px/24px urbane;
	  border-bottom: 2px solid #eee;
	  padding-bottom: 4px;
	}
	.tec-spac-b {
	  display: flex;
	  flex-wrap: wrap;
	  align-items: baseline;
	  justify-content: space-between;
	}
	.tec-spac-b .tec-spac-item {
	  display: flex;
	  flex-direction: column;
	  width: 30%;
	  align-items: baseline;
	  margin-bottom: 24px;
	}
	.tec-spac-item2 {
	  justify-content: space-between;
	}
	.tec-spac-item2 p {
	  font: normal normal 300 12px/17px urbane;
	  margin: 0;
	  margin-top: 12px;
	  color: #161c1f;
	  width: 30%;
	}
	.tec-spac-item2 img {
	  width: 66%;
	}
	.tec-spac-b .tec-spac-item h4 {
	  font: normal normal 600 16px/25px urbane;
	  margin: 0;
	  color: #4d4d4d;
	}
	.tec-spac-b .tec-spac-item p {
	  font: normal normal 300 12px/17px urbane;
	  margin: 0;
	  margin-top: 12px;
	  color: #161c1f;
	}
	@media screen and (max-width: 500px) {
	  .order-modal-layer .order-layer-box {
		width: 90vw;
		min-width: auto;
	  }
	  #carbon-tab-s,
	  #carbon-tab-m,
	  #carbon-tab-l {
		flex-direction: column-reverse;
	  }
	  .sizefit-box {
		width: 100%;
	  }
	  .order-modal-layer .sizefit-switch {
		width: calc(100% - 40px);
		margin: 15px auto;
	  }

	  .order-modal-layer .sizefit-switch button span {
		font-size: 12px;
	  }

	  .sizefit-bg {
		flex: inherit;
		height: 190px;
		width: 100%;
		margin-left: 0;
		margin-bottom: 20px;
	  }
	  .tec-spac-b {
		flex-direction: column;
	  }
	  .tec-spac-b .tec-spac-item {
		width: 100%;
	  }

	  .tec-spac-item2 {
		flex-direction: column;
	  }
	  .tec-spac-item2 p {
		width: 100%;
	  }
	  .tec-spac-item2 img {
		width: 100%;
		margin-top: 30px;
	  }
	}
	@keyframes anim-open {
	  0% {
		opacity: 0;
		-webkit-transform: scale3d(1.1, 1.1, 1);
		transform: scale3d(1.1, 1.1, 1);
	  }
	  100% {
		opacity: 1;
		-webkit-transform: scale3d(1, 1, 1);
		transform: scale3d(1, 1, 1);
	  }
	}
@keyframes anim-close-out {
	  0% {
		opacity: 1;
	  }

	  100% {
		opacity: 0;
	  }
	}
  </style>

