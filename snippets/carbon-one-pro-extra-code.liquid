  <script>
	$('.sizefit-switch .switch-radio input').on('change', e => {
		const parent = $(e.target).closest('.order-modal-layer')

		parent.find('.order-layer-body > div').hide()
		parent.find(`.order-layer-body #carbon-${e.target.id}`).show()
	})

	function onCloseOrderLayer(target) {
		const layer = $(target).closest('.order-modal-layer')

		layer.addClass('dialog-close')
		layer.addClass('dialog-close-out')
		setTimeout(() => {
			layer.removeClass('dialog-close')
			layer.removeClass('dialog-close-out')
			layer.hide()
		}, 300);
	  }
  
	  $('.faq-title').click(function () {
		$(this)
		  .siblings('.faq-content')
		  .slideToggle()
		  .parent()
		  .siblings()
		  .find('.faq-content')
		  .slideUp();
		$(this).children('i').toggleClass('faq-content-plus');
		$(this)
		  .parent()
		  .siblings()
		  .find('.faq-title')
		  .children('i')
		  .addClass('faq-content-plus');
	  });
  </script>