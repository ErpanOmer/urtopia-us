  hideList.forEach((item,index)=>{
	if(item){
	  let {top} = item.getBoundingClientRect();
	  let s=window.innerHeight-top;
	  if(s>0){
		item.classList.add("fade-in-show");
		item=null;
	  }
	}
  })
}
if(window.innerWidth<768)
  {
	
hideList.push(document.querySelector('#bg1-mb'));
hideList.push(document.querySelector('#bg2-mb'));
hideList.push(document.querySelector('#bg3-mb'));
	hideList.push(document.querySelector('#bg4-mb'));
	hideList.push(document.querySelector('#bg5-mb'));
	hideList.push(document.querySelector('#bg6-mb'));
  }
else{
  
hideList.push(document.querySelector('#bg1'));
hideList.push(document.querySelector('#bg2'));
hideList.push(document.querySelector('#bg3'));
  hideList.push(document.querySelector('#bg4'));
  hideList.push(document.querySelector('#bg5'));
}
isShow();</script> </article>
