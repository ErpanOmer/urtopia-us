        $('.carbon1propopup').animate({}, 300, function () {
                //第一个花括号里面是动画内容，可以为空，但不能省去中括号
                $('.carbon1propopup').css({ transform: 'translateY(0)' }); //在回调函数里面改变css属性来实现transform中的动画变换
        });

        fetchBuried('websiteclick', `carbon-1p-ebikepage`, { button: 'Reserve NOW' })
    })

    $('.carbon1propopup .popupclose img').on('click', () => {
        document.body.style.position = ''
        document.body.style.top = ''
        // 使 scrollTop 恢复原状
        document.scrollingElement.scrollTop = _scrollTop
        $('.carbon1propopup').animate({}, 300, function () {
                //第一个花括号里面是动画内容，可以为空，但不能省去中括号
                $('.carbon1propopup').css({ transform: 'translateY(100%)' }); //在回调函数里面改变css属性来实现transform中的动画变换
        });
    })
</script>

