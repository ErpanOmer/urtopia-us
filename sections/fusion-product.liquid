    <script>
      let current_index = 1

      function customSetInterval(callback, interval) {
          let lastTime = performance.now(); // 记录当前时间
          let stop = false; // 停止标志

          function tick(currentTime) {
            if (stop) return; // 如果停止，则退出
            if (currentTime - lastTime >= interval) {
              // 如果经过的时间大于等于设定的间隔时间
              callback(); // 执行回调
              lastTime = currentTime; // 更新上次调用的时间
            }
            requestAnimationFrame(tick); // 下一帧继续调用
          }

          requestAnimationFrame(tick); // 启动递归
          return () => { stop = true; }; // 返回一个停止函数
        }

