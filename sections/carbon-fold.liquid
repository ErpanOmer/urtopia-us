<style>
  :root {
    --product-color: #FFDF48;
  }
</style>

{% assign prelo = 'https://cdn.shopify.com/s/files/1/0583/5810/4213/files/20241017-185158.jpg?v=1729162376' %}
{{ 'carbon-fold-product.scss.css' | asset_url | stylesheet_tag: preload: true }}
<link rel="preload" as="image" href="{{ prelo }}">
<link
  rel="preload"
  as="image"
  href="https://cdn.shopify.com/s/files/1/0583/5810/4213/files/20241017-185454.jpg?v=1729162539"
>

<h1 class="visuallyhidden">Urtopia Carbon 1 Pro Ebike - Sleek, Full Carbon, Lightweight, Smart</h1>
<div class="product" data-pf-type="ProductBox">
  <div class="centeral-content">
    <div class="commonComponent pcHide">
      <div class="u20DemiBold_v2">{{ product.title }}</div>
      <div data-pf-type="JudgeMe" class="sc-eZhRLC gtfCKx pf-25_ order-djs mb16">
        <div class="pf-judme">
          {% render 'judgeme_widgets',
            widget_type: 'judgeme_preview_badge',
            jm_style: '',
            concierge_install: false,
            product: product
          %}
        </div>
      </div>
      <div
        class="product-price u24DemiBold_v2 mb12"
      >
        {{ product.price | money_without_trailing_zeros }}
        <span class="u17Medium_v2" style="font-size: 15px;">{{ product.compare_at_price | money }}</span>
        <span class="u14Medium_v2" style="font-size: 13px;"
          >-{{ product.compare_at_price | minus: product.price | money_without_trailing_zeros -}}
        </span>
      </div>
      {% render 'sale' %}
    </div>
    <div class="left-swiper mobileHide">
      <div class="swiper-wrapper">
        <img class="swiper-slide" alt="Urtopia Carbon 1 Pro" src="{{prelo}}">
      </div>
    </div>
    <span></span>
    <div
      class="featured_image mb16"
      style="--swiper-navigation-color: #000;--swiper-theme-color: #000;--swiper-pagination-color: #000;"
    >
      <div class="right-swiper">
        <div class="swiper-wrapper">
          <div class="swiper-slide"><img alt="Urtopia Carbon 1 Pro" src="{{prelo}}"></div>
        </div>
        <div class="arrow swiper-button-prev mobileHide"></div>
        <div class="arrow swiper-button-next mobileHide"></div>
      </div>
      <div class="medias">
        <i class="u17DemiBold_v2">Carbon Fiber Expert</i>
        <img
          class="lozad"
          data-src="https://newurtopia.com/cdn/shop/files/logo_0707__1.png?v=1684310857&width=500"
          alt=""
        >
      </div>
      <div class="swiper-pagination pcHide"></div>
      <div class="free mobileHide">
        <img
          class="lozad"
          data-src="https://cdn.shopify.com/s/files/1/0583/5810/4213/files/20241008-180903_2x_38f35da3-10a5-404a-a8e3-45502019b10d.png?v=1729068869"
          alt=""
        >
        <span class="u14Medium_v2"
          ><span style="color: var(--product-color);">Limited offer:</span> Fenders, kickstand</span
        >
        <span class="flex-1"></span>
        <del class="u14Light_v2">{{ 16700 | money }}</del>
        <span class="u17Medium_v2" style="color: #000;background-color: var(--product-color);">Free</span>
      </div>
      <div class="icons mobileHide">
