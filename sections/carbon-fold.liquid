<script>
  // 默认隐藏
  $('.product .error-tip').hide();
  // 默认隐藏
  $('.bottom-fixed').hide();
  $('.top-fixed').hide();

  const leftSwiper = new Swiper('.product .left-swiper', {
    direction: 'vertical',
    spaceBetween: 15,
    slidesPerView: 9,
    watchSlidesProgress: true,
    // height: 360,
  });

  const rightSwiper = new Swiper('.product .right-swiper', {
    navigation: {
      nextEl: '.product .featured_image .swiper-button-next',
      prevEl: '.product .featured_image .swiper-button-prev',
    },
    spaceBetween: 10,
    pagination: { el: '.swiper-pagination' },
    thumbs: {
      swiper: leftSwiper,
    },
    roundLengths : true,
    on:{
        slideChange: function() {
            const index = this.activeIndex % global_config.carbon_fold_order_page_config.product_media_images.length;
            $('.product .medias').find('.u17DemiBold_v2').text(global_config.carbon_fold_order_page_config.product_media_images[index].text)
            $('.product .medias').find('img').attr('src', global_config.carbon_fold_order_page_config.product_media_images[index].img)

            if (global_config.carbon_fold_order_page_config.product_media_images[index + 1]) {
                (new Image()).src = global_config.carbon_fold_order_page_config.product_media_images[index + 1].img
            }

            if (global_config.carbon_fold_order_page_config.product_media_images[index + 2]) {
                (new Image()).src = global_config.carbon_fold_order_page_config.product_media_images[index + 2].img
            }            
        },
    },
  });

  document.addEventListener('onChangeSize', (e) => {
    $(`.product .product-panel .options > div[option="Size"] .u17DemiBold_v2 .u17Light_v2`).text(
      global_config.carbon_fold_order_page_config.size_map[e.value]
    );
  });

  document.addEventListener('onChangeColor', (e) => {
    $(`.product .product-panel .options > div[option="Color"] .u17DemiBold_v2 .u17Light_v2`).text(e.value);

    let swiper_arr = [];
    swiper_arr = swiper_arr.concat(
      global_config.carbon_fold_order_page_config.product_images[e.value],
      global_config.carbon_fold_order_page_config.product_images['commonSwiper']
    );

    console.log(swiper_arr);
    rightSwiper.removeAllSlides()
    leftSwiper.removeAllSlides()

    for (let i = 0; i < swiper_arr.length; i++) {
      rightSwiper.appendSlide(`<div class="swiper-slide"><img alt="Urtopia Carbon 1 Pro" src="${swiper_arr[i]}"/></div>`)
      leftSwiper.appendSlide( `<img class="swiper-slide" alt="Urtopia Carbon 1 Pro" src="${swiper_arr[i]}"/>`)
    }
  });

  document.addEventListener('onChangeOptions', (e) => {
    $('.product .delivery_time').html(
      global_config.carbon_fold_order_page_config.ebike_delivery_time[e.value.id] ||
        global_config.carbon_fold_order_page_config.ebike_default_delivery_time
    );
  });

  $('.product .product-panel').on('submit', function (event) {
    // 阻止默认行为
    event.preventDefault();
    return false;
  });

  $('.product .dialogs > div').on('click', function (e) {
    const m = {
      0: 'one',
      1: 'two',
      2: 'three',
      3: 'four',
    };

    if ($(this).attr('onclick')) {
      return;
    }

    $('.product .dialogs > div').each((i, t) => {
      if ($(e.target).closest('.u17Medium_v2')[0] === t) {
        i = i > 1 ? 3 : i;

        setTimeout(() => {
          const layer = document.querySelector(`.order-modal-${m[i]}`);
          const laybox = document.querySelector(`.order-modal-layer-${m[i]}`);
          layer.style.display = 'flex';
          layer.style.zIndex = '99999';
          laybox.className += ' dialog-open';
        }, 100);
      }
    });
  });

  function changeAddToCartText(parse, step = 0) {
    if (step) {
      $('.addToCartBtn i').toggleClass('showi');
      document.querySelector('.addToCartBtn span').innerHTML = 'Adding...';
    } else {
      setTimeout(() => {
        $('.addToCartBtn i').toggleClass('showi');
        document.querySelector('.addToCartBtn span').innerHTML = 'Add to Cart';
      }, 800);
    }
  }

  let id = 0;
  let adding = false;

  function addAllToCart(parse, show = false) {
    let cart1 = document.querySelector('cart-notification') || document.querySelector('cart-drawer');
    // 如果 B test 需要显示 klarna checkout
    window.show_notification_checkout_button = show;
    const cartList = { items: [] };
    $('.product .error-tip').hide();

    if (adding == false) {
      !show && changeAddToCartText(parse, 1);
      adding = true;

      cartList.items.push({ id: current_variant_id, quantity: 1 });

      if (cart1) {
        var sections = cart1.getSectionsToRender().map((section) => section.id);
        cartList.sections = sections.join(',');
      }

      // 新增普通配件
      $('.product .accessories .items .active').each((i, item) => {
        cartList.items.push({
          id: $(item).attr('variant-id'),
          quantity: 1,
        });
      });

      fetch('/cart/add.js', {
        method: 'POST',
        body: JSON.stringify(cartList),
        headers: {
          'Content-Type': 'application/json',
        },
      })
        .then((res) => res.json())
        .then((res1) => {
          res1.key = '';

          cart1.classList.contains('is-empty') && cart1.classList.remove('is-empty');

          if (!(res1.message && res1.status)) {
            cart1.renderContents(res1);
          } else {
            $('.product .error-tip').show();
          }

          !show && changeAddToCartText(parse, 0);
        })
        .catch((err) => {
          !show && changeAddToCartText(parse, 0);
          // cartList.items.splice(-1, 1);
          console.log('err==', err);
          $('.product .error-tip').show();
        })
        .finally(() => {
          adding = false;
          !show && fetchBuried('websiteclick', `fold1-order-page`, { button: 'ATC' });

          setTimeout(() => $('.product .error-tip').hide(), 10000);
        });
    }
  }

  function testRideTrace(type = '', target) {
    // 如果已经有按钮 loading ,直接退出来
    if (!type || $('.orderTestRideBtn').hasClass('loading')) {
      return;
    }

    const t = $(target);

    t.addClass('loading');
    // 原本的html 存起来
    const text = t.find('span').html();
    const loading = '<i class="fa fa-spinner fa-spin"></i>Processing...';

    // loading
    t.find('span').html(loading);

    fetchBuried('websiteclick', `fold1-order-page`, { button: type }, true);
    window.sessionStorage.clicked_klarna_checkout_button = 'clicked_klarna_checkout_button';

    if (sessionStorage.carbon_one_a_b_test_flag === 'B') {
      setTimeout(() => {
        t.removeClass('loading');
        t.find('span').html(text);
      }, 2000);

      return addAllToCart('atc', true);
    }
  }

  $('.product .product-panel .accessories > div:not(.u17DemiBold_v2):not(.items)').on('click', (e) => {
    const items = $('.product .product-panel .accessories .items');

    items.animate(
      {
        scrollLeft:
          $('.product .product-panel .accessories .items').scrollLeft() + ($(e.target).hasClass('next') ? 160 : -160),
      },
      300
    );
  });

  $('.product .product-panel .accessories .items .item').on('click', (e) => {
    const item = $(e.target).closest('.item');
    const icon = item.find('.u11Light_v2');

    if (item.hasClass('active')) {
      item.removeClass('active');
      icon.html('<i class="fa-solid fa-plus"></i>');
    } else {
      item.addClass('active');
      icon.html('<i class="fa-solid fa-check"></i>');
    }
  });

  let currnetScroolTop = 0;
  $(document).on(
    'scroll',
    throttle(
      (e) => {
        const top1 = $('.makes').offset().top;
        const top3 = $('.product .addToCartBtn').offset().top;
        const top2 = $(e.target).scrollTop();

        const h = window.screen.height;

        if (global_config.is_mobile) {
          if (top2 > top3) {
            $('.bottom-fixed').show();
          } else {
            $('.bottom-fixed').hide();
          }
        } else {
          if (top2 > top1) {
            $('.top-fixed').show();
          } else {
            $('.top-fixed').hide();
          }

          if (currnetScroolTop > top2) {
            $('.top-fixed').hide();
          }

          currnetScroolTop = top2;
        }
      },
      300,
      100
    )
  );
</script>
