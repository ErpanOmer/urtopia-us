            disableOnInteraction: false,
            pauseOnMouseEnter: false,
            delay: 2000,
        },
        loop: true,
        effect: 'fade',
        grabCursor: true,
        fadeEffect: {
            crossFade: true
        },
        speed: 600,
        on: {
          reachEnd: function(){
            $(`.step_folding .progress > div`).removeClass('active')
          },
            slideChange: function () {      
                if (this.realIndex === 0) {
                    // 往右
                    if (this.activeIndex > this.previousIndex) {
                         setTimeout(() => {
                           $(`.step_folding .progress > div:nth-child(1)`).addClass('active')
                         }, 66)
                    } else {
                         setTimeout(() => {
                           $(`.step_folding .progress > div:nth-child(1)`).addClass('active')
                         }, 66)
                      
                        $(`.step_folding .progress > div:nth-child(2)`).removeClass('active')
                        $(`.step_folding .progress > div:nth-child(3)`).removeClass('active')
                    }
                }

                if (this.realIndex === 1) {
                    // 往右
                    if (this.activeIndex > this.previousIndex) {
                        $(`.step_folding .progress > div:nth-child(2)`).addClass('active')
                    } else {
                        $(`.step_folding .progress > div:nth-child(1)`).addClass('active')
                        $(`.step_folding .progress > div:nth-child(2)`).addClass('active')
                        $(`.step_folding .progress > div:nth-child(3)`).removeClass('active')
                    }
                }

                if (this.realIndex === 2) {
                    if (this.activeIndex > this.previousIndex) {
                        $(`.step_folding .progress > div:nth-child(3)`).addClass('active')
                    } else {
                        $(`.step_folding .progress > div`).addClass('active')
                    }                
                }
            },
        },
    });
</script>
