  if(list[index].style.borderColor=="rgb(0, 0, 0)")
	{
	  list[index].style.borderColor = "#E0E0E0";
	  list[index].style.background = "rgba(240,240,240,0)";
	  list[index].style.borderWidth = "1px";
	  list[index].style.padding="10px 15px 10px 1px";
	  accessoriesIconList[index].style.background="#F7F7F7"
	  accessoriesIconList[index].innerHTML = '<i class="fa-solid fa-plus"></i>'
	  var rmI= 0;
	  for(rmI=0;rmI<cartList.items.length;rmI++){
		  if(cartList.items[rmI].id==accessoriesVariant.id)
			{
			  cartList.items.splice(rmI,1);
			  accNameList.splice(rmI,1);
			  break;
		  }
	  }
	  totalPrice -= accessoriesVariant.price;
