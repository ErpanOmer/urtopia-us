<div class="centeral-content findbikeshop">
  <div class="background-image">
    {% render:"image", width: 1440, mb_src: "https://cdn.shopify.com/s/files/1/0583/5810/4213/files/25013_2x_d61ce1ec-b2d5-4eb9-ab13-beab0401abad.jpg?v=1731494043", src: "https://cdn.shopify.com/s/files/1/0583/5810/4213/files/25017_2x_fe3a5b63-aa22-4093-9e56-9f3120d86873.jpg?v=1731479648" %}
    <h3 class="u30DemiBold_v2">
      Find a Bike Shop Near You <br>
      Same Deal with
      <span style="color:#ffdf48;"
        ><br class="pcHide">
        FREE ASSEMBLY</span
      >
    </h3>
    <p class="u17Light_v2">
      <span>
        With nearly 300 Urtopia dealers across the US, we’ve handpicked over 10 exceptional stores to provide
        outstanding service. If one is nearby, you can enjoy perfect service with free assembly!
      </span>
    </p>
    <a href="/pages/test-ride" target="_blank" class="my-button my-button-origin">Find a bike shop</a>
  </div>
</div>

<div class="professional test-ride">
  <div class="centeral-content">
    <div class="videos swiper-wrapper">
      {% for item in shop.metaobjects.shop_config.test_ride_list.value.value %}
        <div class="swiper-slide" data-url="{{ item.url }}">
          <img loading="lazy" width="269" height="143" src="{{ item.image }}" alt="{{ page_description }}">
          <span class="u20DemiBold_v2">{{ item.title }}</span>
          <span class="u17Light_v2" style="margin: 0;">{{ item.span }}</span>
          <a class="my-link" href="tel:{{ item.tel }}">{{ item.tel }}</a>
        </div>
      {% endfor %}
    </div>
    <div class="swiper-scrollbar"></div>
    <div class="buttons mobileHide">
      <div class="swiper-button-prev"></div>
      <div class="swiper-button-next"></div>
    </div>
  </div>
</div>

<div class="centeral-content win">
  <div>
    <div>
      <h3 class="u30DemiBold_v2">Win a Urtopia E-Bike on CBS’s Show Let’s Make a Deal</h3>
      <span class="u17Light_v2">
        Lights, cameras, action! Urtopia e-bikes hit CBS’s Let’s Make a Deal as the ultimate grand prize for lucky
        contestants. Featured by over 40 networks like ABC and NBC, CBS now joins in celebrating Urtopia. Why did this
        iconic show choose Urtopia? Let’s find out!
      </span>
      <a
        target="_blank"
        class="my-button my-button-origin"
        href="/blogs/blog/win-a-urtopia-e-bike-on-cbss-hit-show-let-s-make-a-deal"
        >Read the Blog</a
      >
    </div>
    <img
      loading="lazy"
      width="450"
      height="357"
      src="https://cdn.shopify.com/s/files/1/0583/5810/4213/files/25141_2x_aafa8a49-b976-4f74-9721-790bf82817bd.png?v=1732698657"
      alt="{{ page_description }}"
    >
  </div>
</div>

<script type="module">
    new Swiper(".swiperrrr .top", {
      spaceBetween: 10,
      preloadImages: false,
      thumbs: global_config.is_mobile
          ? null
          : {
              swiper: new Swiper(".swiperrrr .bottom", {
                  spaceBetween: 12,
                  slidesPerView: 4,
                  freeMode: true,
                  watchSlidesProgress: true,
                  preloadImages: false,
                  // navigation: {
                  //     nextEl: ".swiperrrr .swiper-button-next",
                  //     prevEl: ".swiperrrr .swiper-button-prev",
                  // },
              }),
          },
  });


  new Swiper('#professional .centeral-content', {
    slidesPerView: global_config.is_mobile ? 1.25 : 3.5,
    spaceBetween: 20,
    navigation: {
    nextEl: '#professional .centeral-content .swiper-button-next',
    prevEl: '#professional .centeral-content .swiper-button-prev',
  },
    scrollbar: {
      el: '#professional .centeral-content .swiper-scrollbar',
      draggable: true,
    },
  });

   new Swiper('.test-ride .centeral-content', {
          slidesPerView: global_config.is_mobile ? 1.35 : 4.5,
          spaceBetween: 20,
          navigation: {
          nextEl: '.test-ride .centeral-content .swiper-button-next',
          prevEl: '.test-ride .centeral-content .swiper-button-prev',
          },
          scrollbar: {
              el: '.test-ride .centeral-content .swiper-scrollbar',
              draggable: true,
          },
     });

     $('.test-ride .centeral-content .swiper-slide').on('click', function () {
        window.open($(this).attr('data-url'))
     })

   $('.product-card .dots > div').on('click', e => {
        const target = $(e.target)
        const product_card = target.closest('.product-card')

        // 先取消 其它兄弟元素的active, 再设置active
        target.siblings().removeClass('active')
        target.addClass('active')

        const price = Number(target.attr('data-price'))
        const compare_at_price = Number(target.attr('data-compare-at-price'))
        const discount_price = Number(target.attr('data-product-discount-price'))
        const variant_id = target.attr('data-variant')
        const variant_image = target.attr('data-img')

        product_card.find('.product_price .u24DemiBold_v2').text(getPriceFormat(price, true)).attr('data-price', getPriceFormat(compare_at_price, true))
        product_card.find('.tags > span:first-child').html(`${getPriceFormat(compare_at_price - price, true)} <span class="u14DemiBold_v2">OFF</span>`)

        if (price === compare_at_price) {
            product_card.find('.product_price .u24DemiBold_v2').attr('data-price', '')
            product_card.find('.tags > span:first-child').hide()
        }

        product_card.find('.variant-image').attr('src', variant_image)
        product_card.find('.my-button').attr('href', `${product_card.find('.my-button').attr('href').split('?').shift()}?variant=${variant_id}`)
        product_card.find('.stars a').attr('href', `${product_card.find('.my-button').attr('href').split('?').shift()}?variant=${variant_id}#judgeme_product_reviews`)
    })

    setTimeout(() => {
        $('.product-card .dots > div:first-child').click()

        $('.product-card .variant-image').on('click', e => {
            const button = $(e.target).closest('.product-card').find('.my-button')

            if (button.length) {
                button[0].click()
            }
        })
    })
</script>
