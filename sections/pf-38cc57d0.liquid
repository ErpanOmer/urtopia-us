<script>
    var adding = false;
    var id = 0;
  	var quantity = 1;
    var cart1 =document.querySelector("cart-notification") || document.querySelector("cart-drawer");
    function addToCartAC(){
        if (adding == false) {
            var acProduct = { items: [],sections: {} };
            document.querySelector(".addToCartText").innerHTML = "Adding";
            adding = true;
            var form = document.querySelector("form#product_form_8099381772536");
            let formData = new FormData(form);
            id = formData.get("id");
		  	quantity = formData.get("quantity")
            acProduct.items.push({ id: id, quantity: quantity });
            var sections = cart1.getSectionsToRender().map((section) => section.id);
            acProduct.sections = sections.join(",");
            fetch("/cart/add.js", {
                method: "POST",
                body: JSON.stringify(acProduct),
                headers: {
                    "Content-Type": "application/json",
                },
            })
                .then((res) => res.json())
                .then((res1) => {
                    res1.key = "";
                    cart1.renderContents(res1);
                    document.querySelector(".addToCartText").innerHTML = "Add to Cart";
                    adding = false;
                    acProduct.items=[];
                })
                .catch((err) => {
                    document.querySelector(".addToCartText").innerHTML = "Add to Cart";
                    adding = false;
                });
        }
    }
</script>
