<script>
  
var id = 0;
var adding = false;
var cart1 =
  document.querySelector("cart-notification") ||
  document.querySelector("cart-drawer");
    function changeAddToCartText(type, step) {
	  if(step==1){
        if (type == "buynow") {
			$(".buyNowBtn i").toggleClass("showi");
			document.querySelector(".buyNowBtn span").innerHTML = "Processing";
        }
        else if (type == "atc") {
			$(".addToCartBtn i").toggleClass("showi");
            document.querySelector(".addToCartBtn span").innerHTML = "Adding";
        }
	  }
	  else{
        if (type == "buynow") {
			document.querySelector(".buyNowBtn span").innerHTML = "Buy Now";
			$(".buyNowBtn i").toggleClass("showi");
        }
        else if (type == "atc") {
		  setTimeout(()=>{
			$(".addToCartBtn i").toggleClass("showi");
            document.querySelector(".addToCartBtn span").innerHTML = "Add to Cart";
		  	adding = false;
		  },800)
        }
	  }
    }
  
function addAllToCart(parse) {
  if (adding == false) {
    changeAddToCartText(parse,1);
    adding = true;
    var form = document.querySelector("form#product_form_7982118142200");
    let formData = new FormData(form);
    id = formData.get("id");
    cartList.items.push({ id: id, quantity: 1 });
	if(cart1){
    	var sections = cart1.getSectionsToRender().map((section) => section.id);
    	cartList.sections = sections.join(",");
	}
    fetch("/cart/add.js", {
      method: "POST",
      body: JSON.stringify(cartList),
      headers: {
        "Content-Type": "application/json",
      },
    })
      .then((res) => res.json())
      .then((res1) => {
        res1.key = "";
        cartList.items.splice(-1, 1);
		let body = {
		  trace_name: "order-pc"+parse
  		}	
		if(window.innerWidth <=768){
			body.trace_name = "order-mb"+parse;
		}
		fetch("https://api.newurtopia.com/third_part/book_ride/traces", {
		  method: "POST",
		  body: JSON.stringify(body),
		  headers: {
			"Content-Type": "application/json",
		  },
		})
	  	
		if(parse=="buynow"){
		  window.location.href="https://newurtopia.com/checkout"
		}else{
		  cart1.renderContents(res1);
		  changeAddToCartText(parse, 0);
		}
      })
      .catch((err) => {
       	changeAddToCartText(parse, 0);
        cartList.items.splice(-1, 1);
		console.log('err==',err)
		$(".error-tip").css("visibility","visible");
		setTimeout(()=>{
			  $(".error-tip").css("visibility","hidden");
		},5000)
      });
  }
}
  
</script>
