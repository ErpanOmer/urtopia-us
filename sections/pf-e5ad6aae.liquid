	  $("#splide01-track").append(mediaPrevBtn,mediaNextBtn)
		$(".splide--ttb .splide__list .splide__slide").eq(0).addClass("active")
	  let preBtnDom =$(".media-prev-btn");
	  let nextBtnDom =$(".media-next-btn");
	  if(mainMediaSplide.index==0){
		preBtnDom.addClass("hidden-btn");
	  }else if(mainMediaSplide.index==5){
		nextBtnDom.addClass("hidden-btn");
	  }
	  preBtnDom.on("click",()=>{
		mainMediaSplide.go("<")
	  })
	  nextBtnDom.on("click",()=>{
		mainMediaSplide.go(">")
	  })
	  mainMediaSplide.on("move",(n,p)=>{
		if(p==5){
			nextBtnDom.removeClass("hidden-btn");
		}else if(p==0){
			preBtnDom.removeClass("hidden-btn");
		}
		if(n==0){
			preBtnDom.addClass("hidden-btn");
		}else if(n==5){
			nextBtnDom.addClass("hidden-btn");
		}
		if(p<=5 && n>5){
		  preBtnDom.off("click").on("click",()=>{
			mainMediaSplide.go(">0")
		  })
		  nextBtnDom.off("click").on("click",()=>{
			mainMediaSplide.go(">0")
		  })
		}else if(n<=5 && p>5){
		  preBtnDom.off("click").on("click",()=>{
			mainMediaSplide.go("<")
		  })
		  nextBtnDom.off("click").on("click",()=>{
			mainMediaSplide.go(">")
		  })
		}
	  })
	  mainMediaSplide.on("moved",(n,p)=>{
		$(".splide--ttb .splide__list .splide__slide").removeClass("active")
		$(".splide--ttb .splide__list .splide__slide").eq(n).addClass("active")
		if(![0,1,2,3,4].includes(n)){
		   return;
		}
		var container = $(".brand-judge .left-container .container");
		container.css("transition", "");
		container.css("opacity", "0");
		container.css("transform", "translateY(20%)");
		
		let des =$(".brand-judge .left-container .judge-des")
		let img =$(".brand-judge .left-container .judge-img")
		switch (n) {
		  case 0:
			des.html("“Best Electric Bike for 2022”");
img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/cnet-1671527740909.png")
			break;
		  case 1:
			des.html("“One of the smartest and lightest ebike”");
img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/img_v2_1f5341a218124ab39b4bb5cd49cc335g-1668500914564.png")
			break;
		  case 2:
			des.html("“Like a phone’s than a next-gen fixie”");img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/mask-group-181222x-1672131955609.png")
			break;
		  case 3:
			des.html("“Does phone tech better than some phones”");
			img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/img_v2_c977d9efae3a4c46ad2ddac55c5dabfg-1668500914568.png")
			break;
		  case 4:
			des.html("“It’s incredibly fun to ride”");
			img.attr("src","https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/20221227174330-1672134242872.png")
			break;
		  default:
		}
		setTimeout(() => {
		  container.css("transition", "all 0.5s");
		  container.css("opacity", "1");
		  container.css("transform", "translateY(0)");
		}, 0)
	  })
	  mainMediaSplide.on("dragged",()=>{
		if(mainMediaSplide.index>4){
		  mainMediaSplide.go(">0")
		}
	  })
	  mainMediaSplide.options = {drag:true,heightRatio:0.751};
	  clearInterval(mediaTimer);
	  mediaTimer=null;
	}
},500)


