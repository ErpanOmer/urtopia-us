//image
let mediaTimer = setInterval(()=>{
  	if(window.mainSlider && window.mainSlider.hasOwnProperty(objectKey)){
	  let mainMediaSplide = window.mainSlider[objectKey];
	  let mediaPrevBtn ='<img class="media-prev-btn" src="https://cdn.shopify.com/s/files/1/0583/5810/4213/t/63/assets/productmedialeftarrow-1666953671542.png">'
